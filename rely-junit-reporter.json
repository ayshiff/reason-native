  "scripts": {
    "release": "node ./scripts/esy-prepublish.js rely-junit-reporter.json"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": "< 4.0.0",
    "ocaml": "^4.4.0",
    "@reason-native/rely": "*",
    "@opam/junit": "*"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*"
  }
}
