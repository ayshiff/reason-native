{
  "name": "@reason-native/rely-junit-reporter",
  "version": "1.0.0",
  "description": "A reporter for logging Rely results to junit",
  "author": "Facebook Engineering",
  "homepage": "https://reason-native.com",
  "esy-prepublish-generate-opam": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/facebookexperimental/reason-native"
  },
  "license": "MIT",
  "keywords": [
    "reason",
    "facebook",
    "jest",
    "runner",
    "test",
    "testing",
    "snapshot",
    "rely",
    "reason-native",
    "native",
    "junit",
    "rely-junit-reporter",
    "ci",
  ],
  "esy": {
    "build": "dune build -p rely-junit-reporter",
